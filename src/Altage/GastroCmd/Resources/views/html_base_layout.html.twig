<!DOCTYPE html>
<html lang="fr">
  <head>
    <meta charset={{ _charset }}>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />
    <meta name="author" content="jowamedia.ch">
    <meta name="robots" content="noindex,nofollow" />
	
    <title>{% block title %}{% endblock %}</title>
    
	<link rel="shortcut icon" sizes="16x16" href="{{ asset('favicon.ico') }}" />
	<!-- bootstrap & fontawesome -->
	<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/bootstrap.min.css') }}" />
	<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/font-awesome.min.css') }}" />
	<!-- page specific plugin styles -->
	<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/jquery-ui.custom.min.css') }}" />
	<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/chosen.css') }}" />
	<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/datepicker.css') }}">
	<!-- text fonts -->
	<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/ace-fonts.css') }}" />
	<!-- ace styles -->
	<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/ace.min.css') }}" />

	<!--[if lte IE 9]>
		<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/ace-part2.min.css') }}" />
		<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/ace-ie.min.css') }}" />
	<![endif]-->
	
	<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/ace-skins.min.css') }}" />
	<link rel="stylesheet" href="{{ asset('bundles/altagegastrocmd/css/ace-rtl.min.css') }}" />

	<!-- inline styles related to this page -->
	{% block style %}{% endblock %}
	<!-- ace settings handler -->
	<script src="{{ asset('bundles/altagegastrocmd/js/ace-extra.min.js') }}"></script>

	<!-- HTML5 shim and Respond.js IE8 support of HTML5 elements and media queries -->
	<!--[if lte IE 8]>
            <script src="{{ asset('bundles/altagegastrocmd/js/html5shiv.js') }}"></script>
            <script src="{{ asset('bundles/altagegastrocmd/js/respond.min.js') }}"></script>
	<![endif]-->
    {% block head %}{% endblock %}
  </head>
	<body class="skin no-skin">
	
		{% block body %}{% endblock %}

		
		<!-- basic scripts -->
		<!--[if !IE]> -->
		<script type="text/javascript">
			window.jQuery || document.write("<script src='{{ asset('bundles/altagegastrocmd/js/jquery.min.js') }}'>"+"<"+"/script>");
		</script>
		<!-- <![endif]-->
		<!--[if IE]>
		<script type="text/javascript">
		 window.jQuery || document.write("<script src='{{ asset('bundles/altagegastrocmd/js/jquery1x.min.js') }}'>"+"<"+"/script>");
		</script>
		<![endif]-->
		<script type="text/javascript">
			if('ontouchstart' in document.documentElement) document.write("<script src='{{ asset('bundles/altagegastrocmd/js/jquery.mobile.custom.min.js') }}'>"+"<"+"/script>");
		</script>
		<script src="{{ asset('bundles/altagegastrocmd/js/bootstrap.min.js') }}"></script>
		<!-- page specific plugin scripts -->

		<!--[if lte IE 8]>
		  <script src="{{ asset('bundles/altagegastrocmd/js/excanvas.min.js') }}"></script>
		<![endif]-->
		<script src="{{ asset('bundles/altagegastrocmd/js/jquery-ui.custom.min.js') }}"></script>
		<script src="{{ asset('bundles/altagegastrocmd/js/jquery.ui.touch-punch.min.js') }}"></script>
		<script src="{{ asset('bundles/altagegastrocmd/js/chosen.jquery.min.js') }}"></script>
		<script src="{{ asset('bundles/altagegastrocmd/js/fuelux/fuelux.spinner.min.js') }}"></script>
		<script src="{{ asset('bundles/altagegastrocmd/js/date-time/bootstrap-datepicker.min.js') }}"></script>
		<script src="{{ asset('bundles/altagegastrocmd/js/jquery.maskedinput.min.js') }}"></script>
		
		<!-- ace scripts -->
		<script src="{{ asset('bundles/altagegastrocmd/js/ace-elements.min.js') }}"></script>
		<script src="{{ asset('bundles/altagegastrocmd/js/ace.min.js') }}"></script>
		
		<!-- inline scripts related to this page -->
		{% block javascript %}{% endblock %}
	</body>
</html>